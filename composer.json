{
    "name": "guennichi/collection",
    "description": "A lightweight PHP >= 8.1 library to create immutable collections of objects",
    "keywords": ["collection", "immutable", "readonly"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Radhi Guennichi",
            "email": "hello@guennichi.com",
            "homepage": "https://www.guennichi.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "friendsofphp/php-cs-fixer": "^3.9",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Guennichi\\Collection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Guennichi\\Collection\\": "tests/"
        }
    }
}
